<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baidu.zhuanche"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	
    <application
        android:allowBackup="true"
        android:name=".base.BaseApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashUI"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.UserLoginSelectUI"/>
        <activity android:name=".ui.UserLoginUI"/>
        <activity android:name=".ui.UserCenterUI"/>
        <activity android:name=".ui.UserInfoUI"/>
        <activity android:name=".ui.UserMessageUI"/>
        <activity android:name=".ui.SettingUI"/>
        <activity android:name=".ui.YuYueDetailUI"/>
        <activity android:name=".ui.UserHomeUI"/>
        <activity android:name=".ui.UserRegistUI"/>
        <activity android:name=".ui.YuyueUI"/>
    </application>

</manifest>
